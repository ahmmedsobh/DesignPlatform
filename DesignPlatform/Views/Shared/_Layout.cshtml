<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DesignPlatform</title>
    

    <!-- Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;500&display=swap" rel="stylesheet">

    <!-- Icons -->
    <link rel="stylesheet" href="~/assets/css/all.css">

    <!-- Swipper css -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />


    <!-- Datepicker css -->
    <link rel="stylesheet" href="~/assets/css/jquery.datetimepicker.css">

    <!-- Custom Style -->
    <link rel="stylesheet" href="~/assets/css/main.css">

</head>
<body>

    <!-- Start NavBar -->
    <nav class="navbar navbar-expand-lg ">
        <div class="container">
            <a class="navbar-brand" href="#">
                <img class="logo" src="~/assets/imgs/logo.png">
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <i class="fa-solid fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active p-2 p-lg-3" aria-current="page" href="#">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" aria-current="page" href="#">Get Started</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" aria-current="page" href="#">How it works</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" aria-current="page" href="#Packages">Services</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" aria-current="page" href="/Home/AboutUs">About us </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" aria-current="page" href="/Home/ContactUs">Contact Us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" aria-current="page" href="#Reviews">Reviews </a>
                    </li>

                </ul>

                <a class="btn main-btn" href="/Customer/Dashboard">Customer Login</a>



            </div>
        </div>
    </nav>
    <!-- End NavBar -->
    <main role="main" class="pb-3">
        @RenderBody()
    </main>

    <!-- Start Footer -->
    <footer>
        <div class="container">

            <div class="row">

                <div class="col-lg-3">
                    <h6>Phone Support</h6>
                    <ul class="link-list">
                        <li><a href="tel:222-555-5555">222-555-5555</a></li>
                        <li><a href="tel:222-555-5555">222-555-5555</a></li>
                        <li> <a href="tel:222-555-5555">222-555-5555</a></li>
                    </ul>

                    <h6>24/7 Email Support</h6>
                    <ul class="link-list">
                        <li> <a href="mailto:support@example.com">support@example.com</a></li>
                    </ul>

                </div>

                <div class="col-lg-3">
                    <ul class="link-list">
                        <li><a href="#">About Us</a></li>
                        <li><a href="#">Contact Us</a></li>
                        <li><a href="#">Terms And Conditions</a></li>
                        <li><a href="#">Galary</a></li>
                        <li><a href="#">Link</a></li>
                        <li><a href="#">Link</a></li>
                        <li><a href="#">Link</a></li>
                    </ul>

                </div>

                <div class="col-lg-3">
                    <ul class="link-list">
                        <li><a href="#">About Us</a></li>
                        <li><a href="#">Contact Us</a></li>
                        <li><a href="#">Terms And Conditions</a></li>
                        <li><a href="#">Galary</a></li>
                        <li><a href="#">Link</a></li>
                        <li><a href="#">Link</a></li>
                        <li><a href="#">Link</a></li>
                    </ul>

                </div>


                <div class="col-lg-3">
                    <h6>Get In Touch</h6>
                    <ul>
                        <li><a href="#"><i class="fa-brands fa-facebook-f"></i></a></li>
                        <li><a href="#"><i class="fa-brands fa-instagram"></i></a></li>
                        <li><a href="#"><i class="fa-brands fa-linkedin-in"></i></a></li>
                    </ul>

                </div>

            </div>

        </div>
    </footer>
    <!-- End Footer -->

    <!-- Bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

    <!-- JQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>

    @*JQuery Validation*@
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>



    <!-- DatePicker Js -->
    <script src="~/assets/js/jquery.datetimepicker.full.min.js"></script>


    <!-- Icons -->
    <script src="~/assets/js/all.js"></script>

    <!-- Swipper JS  -->
    <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>

    <!-- Custom Js -->
    <script src="~/assets/js/main.js"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
